<!--StaticPage-->
<div id="menubar" class="menubar-inverse ">
  <div class="menubar-fixed-panel">
    <div>
      <a class="btn btn-icon-toggle btn-default menubar-toggle" data-toggle="menubar" href="javascript:void(0);">
        <i class="fa fa-bars"></i>
      </a>
    </div>
    <div class="expanded">
      <%= link_to root_path, class:'active' do %>
       <span class="text-lg text-bold text-primary ">OBOOK&nbsp;FRAMGIA</span>
      <% end %>
    </div>
  </div>
  <div class="menubar-scroll-panel">

    <!-- BEGIN MAIN MENU -->
    <ul id="main-menu" class="gui-controls">
      <li>
        <%= link_to root_path, class:'active' do %>
         <div class="gui-icon"><i class="fa fa-clock-o"></i></div>
         <span class="title">Timeline</span>
        <% end %>
      </li><!--end /menu-li -->

      <!-- BEGIN EMAIL -->
      <li>
        <%= link_to book_list_path do %>
          <div class="gui-icon"><i class="md md-book"></i></div>
          <span class="title">Books</span>
        <% end %>
      </li><!--end /menu-li -->
      <!-- END EMAIL -->

      <li>
        <%= link_to history_list_path do %>
          <div class="gui-icon"><i class="fa fa-history"></i></div>
          <span class="title">History</span>
        <% end %>
      </li><!--end /menu-li -->

      <!-- BEGIN UI -->
      <li>
        <%= link_to user_list_path do %>
          <div class="gui-icon"><i class="md md-account-circle"></i></div>
          <span class="title">Users</span>
        <% end %>
      </li><!--end /menu-li -->
      <!-- END UI -->

      <!-- BEGIN UI -->
      <li>
        <%= link_to request_list_path do %>
          <div class="gui-icon"><i class="md md-call"></i></div>
          <span class="title">Requests</span>
        <% end %>
      </li><!--end /menu-li -->
      <!-- END UI -->


      <!-- END LEVELS -->

    </ul><!--end .main-menu -->
    <!-- END MAIN MENU -->

    <div class="menubar-foot-panel">
      <small class="no-linebreak hidden-folded">
        <span class="opacity-75">Copyright &copy; 2014</span> <strong>CodeCovers</strong>
      </small>
    </div>
  </div><!--end .menubar-scroll-panel-->
</div><!--end #menubar-->
